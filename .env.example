# ML Emotion Detection Configuration
JEFFREY_EMO_ML=true                    # Enable ML detection (true/false)
EMO_MODEL_NAME=sentence-transformers/all-MiniLM-L6-v2  # Model to use
EMO_DATA_DIR=data                      # Directory for prototypes.npz
EMO_WARMUP=true                        # Enable warmup on init
EMO_WARMUP_TEXT=test                   # Text for warmup
EMO_TIMEOUT_MS=250                     # Max latency before fallback (ms)
EMO_THREADS=4                          # ThreadPool size
EMO_MAX_TEXT_LENGTH=2000              # Max text length
EMO_FALLBACK_LEGACY=true              # Enable legacy fallback
EMO_ORCHESTRATOR_TIMEOUT=0.5          # Orchestrator timeout (seconds)
EMO_RATE_LIMIT=100                    # API rate limit per minute